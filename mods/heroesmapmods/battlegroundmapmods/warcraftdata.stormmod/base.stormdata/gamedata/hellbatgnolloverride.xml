    <On Terms="Effect.KnightGetHitVOXDummy.Start; At Caster" Send="Create Merc_Hellbat_Vox_Gethit" />
    <On Terms="Behavior.JungleCreepPassive.Off" Send="Create Merc_WC_Gnoll_Vox_Aggro" />
