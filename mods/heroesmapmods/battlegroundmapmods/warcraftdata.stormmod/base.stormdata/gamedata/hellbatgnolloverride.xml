      <Image value="Assets\Textures\storm_ui_ingame_targetinfopanel_unit_wcav_merc_gnoll.dds" />
    </GroupIcon>
  </CActorUnit>
  <CActorUnit id="TerranHellbatDefender" parent="StormMinionStandard" unitName="TerranHellbatDefender">
