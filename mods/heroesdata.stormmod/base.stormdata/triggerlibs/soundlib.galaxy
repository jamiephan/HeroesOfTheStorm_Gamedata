//--------------------------------------------------------------------------------------------------
// Trigger: Hero VO Play Heal Thank You Line Trigger
//--------------------------------------------------------------------------------------------------
bool libSond_gt_HeroVOPlayHealThankYouLineTrigger_Func (bool testConds, bool runActions) {
    // Variable Declarations
    fixed lv_healMinimum;
    int lv_impactPlayer;
    int lv_launchPlayer;
    playergroup lv_vOGroup;

    // Automatic Variable Declarations
    // Variable Initialization
    lv_healMinimum = 12.0;
    lv_impactPlayer = UnitGetOwner(EventPlayerEffectUsedUnitImpact());
    lv_launchPlayer = UnitGetOwner(EventPlayerEffectUsedUnitLaunch());
    lv_vOGroup = PlayerGroupEmpty();

    // Conditions
    if (testConds) {
        if (!((lv_impactPlayer != lv_launchPlayer))) {
            return false;
        }

        if (!((EventPlayerEffectUsedAmountFixed(c_effectAmountHealed, false) > lv_healMinimum))) {
            return false;
        }

        if (!((CatalogFieldValueGetAsInt(c_gameCatalogEffect, EventPlayerEffectUsed(), "Flags[TriggerVoiceResponse]", c_playerAny) == 1))) {
            return false;
        }
    }

    // Actions
    if (!runActions) {
        return true;
    }

    PlayerGroupAdd(lv_vOGroup, lv_impactPlayer);
    PlayerGroupAdd(lv_vOGroup, lv_launchPlayer);
    libSond_gf_HeroVOPlayHealThankYouLine(lv_impactPlayer, lv_vOGroup);
    return true;
}

