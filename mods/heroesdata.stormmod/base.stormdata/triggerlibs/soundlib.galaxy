void libSond_gf_SoundAnnouncerVOPlayAnnouncerVOForPlayer (int lp_event, int lp_player) {
    // Variable Declarations
    string lv_soundID;
    int lv_vOEvent;

    // Automatic Variable Declarations
    // Variable Initialization

    // Implementation
    if ((lp_player > libCore_gv_bALMaxEnginePlayerCount)) {
        return ;
    }

    lv_vOEvent = (lp_event);
    if ((libGame_gv_gameOver == true) && (libGame_gv_players[lp_player].lv_lootContent.lv_loot_AnnouncerPack.lv_announcerVOLines[lv_vOEvent].lv_playAtGameEnd == false) && (lp_event != libCore_ge_AnnouncerVOEvents_MVP) && (lp_event != libCore_ge_AnnouncerVOEvents_MVPVoteEpic) && (lp_event != libCore_ge_AnnouncerVOEvents_MVPVoteLegendary)) {
        return ;
    }

    lv_soundID = libGame_gv_players[lp_player].lv_lootContent.lv_loot_AnnouncerPack.lv_announcerVOLines[lv_vOEvent].lv_soundLink;
    if ((lv_soundID == null)) {
        if ((lp_event == libCore_ge_AnnouncerVOEvents_MVP)) {
            SoundPlayForPlayer(SoundLink("MVP_MVP", -1), c_maxPlayers, PlayerGroupSingle(lp_player), 100.0, 0.0);
        }

        if ((lp_event == libCore_ge_AnnouncerVOEvents_MVPVoteEpic)) {
            SoundPlayForPlayer(SoundLink("MVP_Epic", -1), c_maxPlayers, PlayerGroupSingle(lp_player), 100.0, 0.0);
        }

        if ((lp_event == libCore_ge_AnnouncerVOEvents_MVPVoteLegendary)) {
            SoundPlayForPlayer(SoundLink("MVP_Legendary", -1), c_maxPlayers, PlayerGroupSingle(lp_player), 100.0, 0.0);
        }

        return ;
    }

    SoundPlayForPlayer(SoundLink(lv_soundID, c_soundIndexAny), c_maxPlayers, libCore_gv_playerGroupFromPlayer[lp_player], 100.0, 0.0);
    libGame_gf_SendEventAnnouncerVOInitiated(SoundLastPlayed(), lp_player, (lp_event));
}

