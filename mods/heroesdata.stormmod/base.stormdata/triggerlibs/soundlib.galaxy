    // Implementation
    lv_heroUnit = libGame_gv_players[lp_player].lv_heroUnit;
    if ((lv_heroUnit == null)) {
        return ;
    }

    lv_positionOfHeroUnit = UnitGetPosition(lv_heroUnit);
