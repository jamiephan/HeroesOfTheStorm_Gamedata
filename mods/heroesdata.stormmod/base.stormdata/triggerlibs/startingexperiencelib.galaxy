    libNtve_gf_SetDialogItemTextWriteoutDuration(DialogControlLastCreated(), libStEx_gv_sE_MsgWriteOutDuration, PlayerGroupAll());
    libStEx_gv_sE_HintMsgLabel = DialogControlLastCreated();
}

