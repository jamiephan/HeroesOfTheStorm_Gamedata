bool libStEx_gt_SE_TrainingModeTipSpacebar_Func (bool testConds, bool runActions) {
    // Variable Declarations
    unitgroup lv_watchTowers;
    unit lv_itWatchTower;
    bool lv_cameraOff;
    timer lv_cameraOffTimer;

