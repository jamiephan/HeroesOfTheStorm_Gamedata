            libStEx_gf_SE_TrainingModeTipSetDisplayed(libStEx_ge_SE_TrainingModeTips_Spacebar);
            lv_cameraOff = false;
            Wait(120.0, c_timeGame);
        }

        Wait(0.5, c_timeGame);
    }
    return true;
}

