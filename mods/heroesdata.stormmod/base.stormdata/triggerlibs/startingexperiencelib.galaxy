        lv_itPlayer = 1;
        for ( ; ( (autoB5550F23_ai >= 0 && lv_itPlayer <= autoB5550F23_ae) || (autoB5550F23_ai < 0 && lv_itPlayer >= autoB5550F23_ae) ) ; lv_itPlayer += autoB5550F23_ai ) {
