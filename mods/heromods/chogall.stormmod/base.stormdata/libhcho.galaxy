    else {
        UnitIssueOrder(libGame_gv_players[lv_player].lv_heroUnit, OrderTargetingPoint(AbilityCommand("ChoSurgingFistExecute", 0), libHCHO_gv_surgingFistMousePosition[lv_player]), c_orderQueueReplace);
